  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/*.js",
      "build/**/*.js",
      "public/**/*.js",
      "node_modules/**/*.js"
    ],
    "globals": [
      "Blob",
      "File",
      "XMLHttpRequest",
      "HTMLCanvasElement",
      "FileReader",
      "ReactDOM",
      "WebSocket",
      "act",
      "fetch",
      "describe",
      "it",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "shallow",
      "mount",
      "render",
      "renderer",
      "jest"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverageFrom": [
      "src/components/*.js",
      "src/components/**/*.js",
      "src/applications/*.js",
      "src/applications/**/*.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
