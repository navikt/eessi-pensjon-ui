{
  "name": "eessi-pensjon-ui",
  "version": "1.3.5",
  "license": "MIT",
  "description": "EESSI Pensjon UI",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "npm run build:css && npm run build:js",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "build:js": "cross-env NODE_OPTIONS=--max-old-space-size=4096 react-scripts build",
    "dist": "npm run dist:prod",
    "dist:dev": "cross-env NODE_ENV=development npm run dist:css && npm run dist:jsdev",
    "dist:prod": "cross-env NODE_ENV=production npm run dist:css && npm run dist:jsprod",
    "dist:css": "less-watch-compiler --run-once --config less.json",
    "dist:jsdev": "rm -rf ./dist && webpack --config webpack.dev.js",
    "dist:jsprod": "rm -rf ./dist && webpack --config webpack.prod.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "start": "cross-env NODE_ENV=development npm-run-all -p start:*",
    "start:css": "less-watch-compiler --config less.json",
    "start:js": "cross-env PORT=3001 react-scripts start",
    "test": "cross-env NODE_ENV=test react-scripts test",
    "test:ci": "cross-env NODE_ENV=test react-scripts test --updateSnapshot --watchAll=false",
    "test:coverage": "cross-env NODE_ENV=test react-scripts test --coverage  --watchAll=false"
  },
  "main": "dist/index.js",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/eessi-pensjon-ui.git"
  },
  "private": true,
